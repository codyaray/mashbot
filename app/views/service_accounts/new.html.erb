<h1>New service_account</h1>

<% form_for(@service_account) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :service %>
    <%= f.select :service, [:tumblr, :flickr] %>
  </p>
  <p>
    <%= f.label :login %><br />
    <%= f.text_field :login %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', service_accounts_path %>